        android:id="@+id/textViewhaircon2a"
        android:text="@string/haircon2a"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_width="match_parent"
        android:layout_below="@id/view1"
        android:layout_height="match_parent"
        android:layout_alignBottom="@+id/imageViewhaircon2"
        android:layout_toRightOf="@+id/imageViewhaircon2"
        android:layout_toEndOf="@+id/imageViewhaircon2" />
    <TextView
        android:id="@+id/textViewhaircon2b"
        android:text="@string/haircon2b"
        android:textSize="16sp"
        android:gravity="center"
        android:textColor="@color/black"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/imageViewhaircon2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <View
